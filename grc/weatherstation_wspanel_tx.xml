<?xml version="1.0"?>
<block>
  <name>WS panel TX</name>
  <key>weatherstation_wspanel_tx</key>
  <category>[weatherstation]</category>
  <import>import weatherstation</import>
  <make>weatherstation.wspanel_tx(self.GetWin())
self.Add(self.$(id).panel)</make>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
