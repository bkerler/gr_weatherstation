<?xml version="1.0"?>
<block>
  <name>WS panel</name>
  <key>weatherstation_wspanel</key>
  <category>[weatherstation]</category>
  <import>import weatherstation</import>
  <make>weatherstation.wspanel($debug, self.GetWin())
self.Add(self.$(id).panel)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>    
    <option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
</block>
